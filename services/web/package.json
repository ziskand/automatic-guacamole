{
  "name": "services",
  "description": "monorepo services",
  "version": "0.0.1",
  "scripts": {
    "deploy:firebase": "npm run deploy --prefix firebase",
    "deploy:clean": "rm -fr __sapper__ ./firebase/public",
    "deploy": "run-s deploy:clean export deploy:firebase",
    "postinstall": "run-p firebase:install cy:install",
    "firebase:install": "npm install --prefix firebase",
    "dev": "run-p dev:*",
    "dev:firebase": "npm run dev --prefix firebase",
    "dev:sapper": "sapper dev",
    "build": "sapper build --legacy",
    "export": "NODE_ENV=production sapper export ./firebase/public --legacy --entry /admin",
    "start": "node __sapper__/build"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-url": "^5.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "svelte": "^3.17.3"
  }
}
